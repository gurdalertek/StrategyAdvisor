{
  "name": "strategy-advisor",
  "version": "1.0.0",
  "description": "strategy-advisor server",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node --max-old-space-size=1024 server.js",
    "server": "nodemon --max-old-space-size=1024 server.js --exec babel-node --presets babel-preset-env",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Ahmet Lekesiz",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^5.1.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10"
  },
  "devDependencies": {},
  "engines": {
    "node": "16.x",
    "npm": "7.10.0"
  }
}
